package ccnu.dao;

import java.util.List;

import ccnu.entity.Movie;

public interface MovieDao{
	//get	获取单条数据
	public Movie get(String id);
	public Movie get(Movie news);
	
	//findList	获取所有数据
	public List<Movie> findList(Movie news);
	
	//delete	删除数据(修改数据状态属性/直接删除)
	public int delete(String id);
	public int delete(Movie news);
	
	//insert	插入数据
	public int insert(Movie news);
	
	//update	更新数据
	public int update(Movie news);
	
	
}
